import java.util.*;
public class Lab1b {

  public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
    System.out.print("Please input an integer ");
    int n = in.nextInt();
    System.out.print("Please input a second integer ");
    int k = in.nextInt();
    System.out.println("The two ints were " + n + " and " + k);
    

  }

}